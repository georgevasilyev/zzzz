docker run -d --name grass -h 2-3070-3 -e GRASS_USER=k.shmelyov@dnmntm.bizml.ru -e GRASS_PASS=nmeidwxm4F! mrcolorrain/grass

version: "3.9"
services:
  grass:
    container_name: grass
    hostname: 2-3070-3
    image: mrcolorrain/grass
    environment:
      - GRASS_USER=k.shmelyov@dnmntm.bizml.ru
      - GRASS_PASS=nmeidwxm4F!
    restart: unless-stopped
